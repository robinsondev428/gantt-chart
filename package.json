{
  "name": "@infectoone/vue-ganttastic",
  "version": "3.0.0",
  "description": "A simple and customizable Gantt chart component for Vue.js",
  "author": "Marko Zunic (@infectoone)",
  "scripts": {
    "serve": "vite",
    "build": "run-p build:types build:lib",
    "build:lib": "vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly --isolatedModules false --outFile lib/index.d.ts",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "npm run lint --fix",
    "docs:build": "vuepress build docs",
    "docs:dev": "vuepress dev docs"
  },
  "module": "lib/index.mjs",
  "types": "lib/index.d.ts",
  "files": [
    "lib/index.d.ts",
    "lib/index.mjs",
    "lib/style.css"
  ],
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/node": "^16.11.56",
    "@types/postcss-preset-env": "^7.7.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/tsconfig": "^0.1.3",
    "@vuepress/plugin-search": "2.0.0-beta.50",
    "eslint": "^8.23.0",
    "eslint-plugin-vue": "^9.4.0",
    "npm-run-all": "^4.1.5",
    "postcss-preset-env": "^7.8.0",
    "prettier": "^2.7.1",
    "typescript": "~4.7.4",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.6",
    "vuepress": "2.0.0-beta.50"
  },
  "peerDependencies": {
    "dayjs": "^1.11.5",
    "vue": "^3.2.38"
  },
  "homepage": "https://infectoone.github.io/vue-ganttastic/",
  "keywords": [
    "gantt",
    "chart",
    "bar",
    "diagram",
    "vue",
    "vuejs",
    "ganttastic"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/InfectoOne/vue-ganttastic"
  }
}
